<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -30 </chase-distance-m>

  <view n="100">
    <name>Co Pilote</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.397 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  0.475 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -6.425 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -10 </pitch-offset-deg>
      <default-field-of-view-deg>65</default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Roof Gunner</name>
    <type>lookfrom</type>
