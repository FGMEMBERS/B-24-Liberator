<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <key n="100">
    <name>d</name>
    <desc>Bomb bay door</desc>
    <binding>
      <command>nasal</command>
      <script>b24.doorsystem.crewexport();</script>
    </binding>
  </key>

  <key n="10">
    <name>Ctrl-j</name>
    <desc>Jump signal</desc>
    <binding>
      <command>property-assign</command>
      <property>controls/signal</property>
      <value>true</value>
    </binding>
  </key>

</PropertyList>

